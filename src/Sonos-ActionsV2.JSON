{
  "Browse": {
    "path": "/MediaServer/ContentDirectory/Control",
    "args": {
      "InstanceID": 0,
      "BrowseFlag": "BrowseDirectChildren",
      "Filter": "*",
      "StartingIndex": "0",
      "RequestedCount": "100",
      "SortCriteria": "",
      "ObjectID": "FV:2"
    },
    "returnValueName": "Result",
    "info": "ObjectID: FV:2 = Get the My Sonos is default!"
  },
  "SetAVTransportURI": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "CurrentURI": "",
      "CurrentURIMetaData": ""
    },
    "info": "set the player to specific playing mode: queue (x-rincon-queue:<macaddress>0<port>#0), stream, tv, ...",
    "status": ""
  },
  "Play": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "Speed": 1
    },
    "info": "start playing - prereq is that uri is set (either stream or queue) . Response value is boolean" 
  },
  "RemoveAllTracksFromQueue": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0
    },
    "info": "flush queue",
    "status": "in work"
  },
  "AddURIToQueue": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "EnqueuedURI": "",
      "EnqueuedURIMetaData": "",
      "DesiredFirstTrackNumberEnqueued": 0,
      "EnqueueAsNext": 1
    },
    "info": "Add uri to queue, Enqueued meta data in DIDL light format"
  },
  "Seek": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "Unit": "REL_TIME",
      "Target": "00:01:00"
    },
    "info": "Seek with REL_TIME move to position in current song - type string hh:mm:ss"
  },
  "Seek-delta": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "Unit": "TIME_DELTA",
      "Target": "00:00:15"
    },
    "info": "Seek with TIME_DELTA move to position in current song- type string hh:mm:ss"
  },
  "SetBass": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "DesiredBass": "0"
    },
    "info": "Set bass, value in range -10 ... 10 "
  },
  "GetBass": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "CurrentBass",
    "info": "Type is string"
  },
  "SetTreble": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "DesiredTreble": "0"
    },
    "info": "Set treble, value in range -10 ... 10 "
  },
  "GetTreble": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "CurrentTreble",
    "info": "Type is string"
  },
  "SetEQ": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "EQType": "DialogLevel",
      "DesiredValue": "1"
    },
    "eqTypeValues": [
      "DialogLevel",
      "NightMode",
      "SubGain"
    ],
    "info": "Set EQType and DesiredValue have to be set. DesiredValue depend on EQType - type number"
  },
  "GetEQ": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "EQType": "DialogLevel"
    },
    "returnValueName": "CurrentValue",
    "info": "Type is string"
  },
  "SetCrossfadeMode": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "CrossfadeMode": 1
    },
    "info": "Set CrossfadeMode 0 or 1 - type is number"
  },
  "GetCrossfadeMode": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "CrossfadeMode",
    "info": "will return 0 or 1 - type is string"
  },
  "SetLoudness": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "Channel": "Master",
      "DesiredLoudness": "1"
    },
    "info": "Set Loudness 0 or 1 - type number"
  },
  "GetLoudness": {
    "path": "/MediaRenderer/RenderingControl/Control",
    "argsValueName": "CurrentLoudness",
    "dataType": "string",
    "args": {
      "InstanceID": 0,
      "Channel": "Master"
    },
    "returnValueName": "CurrentLoudness",
    "info": "Get Loudness 0 or 1 - type string"
  },
  "ConfigureSleepTimer": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "NewSleepTimerDuration": "00:05:00"
    },
    "info": "Duration in format hh:mm:ss - type string"
  },
  "GetRemainingSleepTimerDuration": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "RemainingSleepTimerDuration",
    "dataType": "string",
    "info": "returns duration in format hh:mm:ss - type string"
  },
  "CreateStereoPair": {
    "path": "/DeviceProperties/Control",
    "args": {
      "InstanceID": 0,
      "ChannelMapSet": "RINCON_B8E9375831C001400:LF,LF;RINCON_000E58FE3AEA01400:RF,RF"
    },
    "info": "pair stereo speakers, must be same model e.g. Play:1, parameters are uuid, format ${left}:LF,LF;${right}:RF,RF"
  },
  "SeparateStereoPair": {
    "path": "/DeviceProperties/Control",
    "args": {
      "InstanceID": 0,
      "ChannelMapSet": "RINCON_B8E9375831C001400:LF,LF;RINCON_000E58FE3AEA01400:RF,RF"
    },
    "info": "unpair stereo speakers - caution channelMapSet is only as example!!!!!!"
  },
  "GetTransportInfo": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "CurrentTransportState",
    "info": "will return PLAYING, PAUSED_PLAYBACK, ...  CAUTION: Client in group will always return playing. Hint: <currentTransportState> and two more xml items are returnd"
  },
  "GetCurrentTransportActions": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "Actions",
  "info": "returns the currently avaialble actions: Set, Stop, Pause, Play, X_DLNA_SeekTime, Next, X_DLNA_SeekTrackNr"
  },
  "SetRelativeGroupVolume": {
    "path": "/MediaRenderer/GroupRenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "Adjustment": "40"
    },
    "info": "set group relative volume, number/integer in range -99 ... -1 or +1 .. +99. returns NewVolume"
  },
  "GetGroupVolume": {
    "path": "/MediaRenderer/GroupRenderingControl/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "CurrentVolume",
  "info": "returns the current group volume "
  },
  "SetGroupMute": {
    "path": "/MediaRenderer/GroupRenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "DesiredMute": "1"
    },
    "info": "set group mute to on (1) or off (0)"
  },
  "GetGroupMute": {
    "path": "/MediaRenderer/GroupRenderingControl/Control",
    "args": {
      "InstanceID": 0
    },
    "returnValueName": "CurrentMute",
  "info": "get group mute on (1) or off (0)"
  }, 
  "SnapshotGroupVolume": {
    "path": "/MediaRenderer/GroupRenderingControl/Control",
    "args": {
      "InstanceID": 0
    },
    "info": "snapshots the current volume relation in group for usage of set relative group volume"
  },
  "SetGroupVolume": {
    "path": "/MediaRenderer/GroupRenderingControl/Control",
    "args": {
      "InstanceID": 0,
      "DesiredVolume": 10
    },
    "info": "set the specified volume to all players"
  },
  "SaveQueue": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "Title": "unknown",
      "ObjectID": ""
    },
    "info": "Save queue to SONOS playlist"
  },
  "DelegateGroupCoordinationTo": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0,
      "NewCoordinator": "RINCON_xxxxx01400",
      "RejoinGroup": true
    },
    "info": "player will delegate coordination and optionally leave group (Rejoingroup = false). NewCoordinator must exist!"
  }, 
  "BecomeCoordinatorOfStandaloneGroup": {
    "path": "/MediaRenderer/AVTransport/Control",
    "args": {
      "InstanceID": 0
    },
    "info": "player becomes a stand alone group"
  },
  "AddMember": {
    "path": "/GroupManagement/Control",
    "args": {
      "InstanceID": 0,
      "MemberID": ""
    },
    "info": "adds a member to that device. MemberID is the newly added device. Should be send to coordinator!"
  }
}
